{
  "name": "Supreme Autocheckout",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Extension designed to enable different modes of add to cart and auto checkout for supreme webstore.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "sessions",
    "background"
  ],
  "content_scripts": [{
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }]
}